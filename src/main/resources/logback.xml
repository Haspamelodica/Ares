<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="STDOUT" class="de.tum.in.test.api.security.FixSystemErrAppender">
    <encoder>
      <pattern>[%level] [%thread] %msg%n</pattern>
    </encoder>
  </appender>

  <root level="debug">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>